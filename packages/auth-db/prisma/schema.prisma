
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
id String @id @default(uuid())
  email String @unique
  hash String
  hashedRt String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
@@map("users")
}